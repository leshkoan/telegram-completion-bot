# Telegram Bot
BOT_TOKEN=your_telegram_bot_token_here

# Ollama (локальный сервер, не требует прокси)
OLLAMA_HOST=http://localhost:11434
AI_MODEL=llama3:8b

# Параметры генерации
MAX_TOKENS=40
TEMPERATURE=0.7
TOP_P=0.9

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=5

# Логирование
LOG_LEVEL=INFO

# Прокси для Telegram API (опционально, если Telegram заблокирован)
USE_PROXY=false
PROXY_URL=                         # Оставить пустым если USE_PROXY=false
# Примеры:
# PROXY_URL=socks5://proxy.example.com:1080
# PROXY_URL=http://proxy.example.com:8080
# PROXY_URL=socks5://username:password@proxy.example.com:1080
